{
    "name": "isifnet/pieadmin",
    "description": "Pie Admin",
    "type": "library",
    "keywords": [ "laravel", "admin", "grid", "form", "laravel admin"],
    "homepage": "https://github.com/isifnet/pieadmin",
    "license": "MIT",
    "authors": [
        {
            "name": "ISIF Support",
            "email": "support@isif.net"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.5|~6.0|~7.0|~8.0|~9.0|~10.0",
        "spatie/eloquent-sortable": "3.*|4.*",
        "doctrine/dbal": "^2.6|^3.0"
    },
    "require-dev": {
        "laravel/dusk": "~5.9|~6",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^10.1",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Isifnet\\PieAdmin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Isifnet\\PieAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Isifnet\\PieAdmin\\AdminServiceProvider"
            ]
        }
    }
}
